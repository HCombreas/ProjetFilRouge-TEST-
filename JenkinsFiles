node {


            stage('SCM') {
                checkout scm
            }
            stage('SonarQube Analysis') {
                def scannerHome = tool 'SonarqubeScanner'
                withSonarQubeEnv() {
                    sh "${scannerHome}/bin/sonar-scanner"
                }
            }
            stage('Build') {
            steps {
                sh 'apt-get install npm'
                sh 'export PATH=$PATH:/usr/bin/npm'
            }
}